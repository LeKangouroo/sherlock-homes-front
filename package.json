{
  "name": "sherlock-homes-front",
  "description": "The front end for the Sherlock Homes search engine",
  "version": "0.0.1",
  "author": "Itadakimas",
  "engines": {
    "node": "8"
  },
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.4",
    "babel-core": "6.17.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "browser-sync": "2.18.13",
    "chai": "4.1.2",
    "chalk": "2.1.0",
    "core-js": "2.5.1",
    "del": "3.0.0",
    "errno": "0.1.4",
    "glob": "7.1.2",
    "gridlex": "2.4.1",
    "gulp": "3.9.1",
    "gulp-eslint": "3.0.1",
    "gulp-gzip": "1.4.0",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "3.3.0",
    "gulp-mocha": "3.0.1",
    "gulp-postcss": "6.2.0",
    "gulp-replace-task": "0.11.0",
    "gulp-sass": "3.1.0",
    "gulp-svg-sprite": "1.3.6",
    "gulp-tar": "1.9.0",
    "gulp-todo": "5.0.0",
    "gulp-util": "3.0.7",
    "gulp-watch": "4.3.10",
    "imagemin-jpeg-recompress": "5.1.0",
    "imagemin-pngquant": "5.0.1",
    "json-loader": "0.5.7",
    "json-server": "0.12.0",
    "jump.js": "1.0.2",
    "lodash": "4.17.4",
    "mocha": "3.5.1",
    "node-notifier": "5.1.2",
    "nodemailer": "4.1.0",
    "normalize.css": "7.0.0",
    "preprocess": "3.1.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "rlite-router": "2.0.3",
    "run-sequence": "2.1.0",
    "selenium-webdriver": "3.5.0",
    "standard-version": "4.2.0",
    "strip-loader": "0.1.2",
    "svg4everybody": "2.1.9",
    "through2": "2.0.3",
    "urijs": "1.18.12",
    "whatwg-fetch": "2.0.3",
    "webpack": "3.5.6",
    "webpack-stream": "3.2.0",
    "yargs": "8.0.2"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  },
  "scripts": {
    "api": "gulp --gulpfile taskrunner/gulpfile.babel.js api",
    "build:preprod": "npm run fclean && gulp --gulpfile taskrunner/gulpfile.babel.js build --env=pre-production --mode=distributable",
    "build:prod": "npm run fclean && gulp --gulpfile taskrunner/gulpfile.babel.js build --env=production --mode=distributable",
    "clean": "gulp --gulpfile taskrunner/gulpfile.babel.js clean",
    "dev": "npm run clean && gulp --gulpfile taskrunner/gulpfile.babel.js dev",
    "email": "gulp --gulpfile taskrunner/gulpfile.babel.js email",
    "fclean": "npm run clean -- --dist",
    "help": "gulp --gulpfile taskrunner/gulpfile.babel.js --help",
    "release": "standard-version",
    "test": "npm run test:unit && npm run test:instrumented",
    "test:instrumented": "gulp --gulpfile taskrunner/gulpfile.babel.js instrumented-tests",
    "test:unit": "gulp --gulpfile taskrunner/gulpfile.babel.js unit-tests",
    "todos": "gulp --gulpfile taskrunner/gulpfile.babel.js todos",
    "zip": "gulp --gulpfile taskrunner/gulpfile.babel.js zip"
  }
}
